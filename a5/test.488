{
    var result : integer

    function const (v : integer) : integer
    {
        return with v
    }

    function bar : integer
    {
        return with const(2)
    }

    procedure add(a : integer, b : integer)
    {
        result := a + b
    }

    procedure foo
    {
        add(2, 3)
        result := result * bar
    }

    foo
    write result
}
