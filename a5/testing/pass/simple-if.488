{
	% simple-if.488: test to see if simple sf statement works as intended
	% Author: KyoKeun Park

	var i, j: integer
	var a, b: boolean

	a := true
	b := false

	if a = b then
		i := 1
	else
		i := 0

	if a then
		j := 1
	else
		j := 0

	if i < j then
		write "Result: pass", newline
	else
		write "Result: fail", newline
}
