{
	% or.488: check the correctness of or operation
	% Author: KyoKeun Park

	var i, j, k : integer
	var a, b, c : boolean

	i := 1
	j := 2
	k := 3

	a := ( i = 1 or j = 1 or k = 1 ) % should be true
	b := ( i = 3 or j = 1 or k = 2 ) % should be false
	c := ( i = 0 or j = 1 or k = 3 ) % should be true

	if (a and not b and c) then
		write "Result: pass", newline
	else
		write "Result: fail", newline
}
