{
    % A1a.488: Arithmetic, logical and comparison operators
    % Author: Jasmeet Brar

    var a : integer
    var b : integer
    var c : integer
    var d : integer

    a := 5
    b := 10

    a := a + 2
    b := b - 12
    c := a * b
    d := 10 / b

    write "Result: ", a, " ", b, " ", c, " ", d, newline
}
