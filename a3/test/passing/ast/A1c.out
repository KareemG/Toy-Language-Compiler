 procedure wait_for_numbers(a : integer, b : integer, c : integer, d : integer) { 
 var temp : integer
 var running : boolean
 running : = true
 while running do
 { 
 read temp
 if (temp) = (a) then
 { 
 exit
 } else
 if (temp) = (b) then
 { 
 exit 1
 } else
 if (temp) = (c) then
 { 
 running : = false
 } else
 if (temp) = (d) then
 { 
 return
 } end
 end
 end
 end
 } 
 } 
 function break_out_of_loop : boolean { 
