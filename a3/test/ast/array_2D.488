
{
    var i, j : integer
    var arr1[5, 5] : integer
    var arr2[-5 .. 5, -5 .. 5] : integer

    i := 1
    while i <= 5 do
    {
        j := 1
        while j <= 5 do
        {
            read arr1[i, j]
            j := j + 1
        }
        i := i + 1
    }

    i := -5
    while i <= 5 do
    {
        j := -5
        while j <= 5 do
        {
            read arr2[i, j]
            j := j + 1
        }
        i := i + 1
    }
}