	procedure PrintHelloWorld
	{
		write "Hello World!"
		write newline
	}
	
	function GetCourseCode(is_grad: boolean) : integer
	{
		if is_grad then
			return 2107
		else
			return 488
	}
	
	procedure PrintCourseGreeting
	{
		write "Welcome to CSC", GetCourseName(false)
		write "/CSC", GetCourseName(true), "!"
		write newline
	}
	
	PrintHelloWorld
	PrintCourseGreeting
}
