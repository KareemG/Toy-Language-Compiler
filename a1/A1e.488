{
    % A1e.488: Recursive functions and procedures
    % Author: KyoKeun Park

    procedure proc(a : integer, b : integer)
    {
        procedure proc2(c : integer)
        {
            proc2(c)
        }
        proc2(a - 1)
    }

    function f1(n : integer) : integer
    {
        function f2(m : integer) : integer
        {
            f2(m - 1)
            return with m
        }
        var a : integer
        a := f2(n)
        return with a
    }

    procedure proc3
    {
        write "hello world"
        proc2

        return
    }

    function f3 : integer
    {
        return with f1(1000)
    }

    proc3
    proc(1, 2)
    write f3
}
